using System;
namespace ConsoleApp1
{
    class Program
    {
        static void Main(string[] args)
        {
            float nota;
            Console.WriteLine("Insira a nota que obteve no exame: ");
            nota = Convert.ToSingle(Console.ReadLine());
            if (nota >= 9.5)
                Console.WriteLine("Aprovado.");
        }
    }
}
